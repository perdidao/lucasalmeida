        @code
            Dim orcamentoSucesso As String = ""
            Dim orcamentoErro As String = ""
            If Request.HttpMethod.ToLower = "post" And Request("formOrcamento") = "formOrcamento" Then
                Dim o As New NsvcContato.Orcamento
                o.Tipo = Request("tipo")
                o.Arquivo = Request("token")
                o.Nome = Request("Nome")
                o.CPF_CPNJ = Request("cpfCnpj")
                o.Endereco = Request("endereco")
                o.Complemento = Request("complemento")
                o.Bairro = Request("bairro")
                o.Cidade = Request("cidade")
                o.UF = Request("estado")
                o.CEP = Request("cep")
                o.Telefone = Request("telefone")
                o.Email = Request("email")
                o.NumPaginas = Request("numpag")
                o.Idioma = Request("idioma")
                o.ServicosAgregados = Request("servAgre").Replace(",", "<br>")
                If (New svcContato).Orcamento(o) Then
                    orcamentoSucesso = "Pedido de orçament enviado com sucesso"
                Else
                    orcamentoErro = "Falha ao enviar pedido de orçamento"
                End If
            End If
        End Code
